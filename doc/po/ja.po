msgid ""
"Here is an example test case:\n"
"  Test::Unit.at_start do\n"
"    # ...\n"
"  end"
msgstr ""

# Test::Unit.at_start
msgid ""
"  class TestMyClass1 < Test::Unit::TestCase\n"
"    class << self\n"
"      def startup\n"
"        # ...\n"
"      end\n"
"    end"
msgstr ""

# Test::Unit.at_start
msgid ""
"    def setup\n"
"      # ...\n"
"    end"
msgstr ""

# Test::Unit.at_exit
# Test::Unit.at_start
msgid ""
"    def test_my_class1\n"
"      # ...\n"
"    end"
msgstr ""

# Test::Unit.at_exit
# Test::Unit.at_start
